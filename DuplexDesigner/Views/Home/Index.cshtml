@{
    ViewBag.Title = "Home-Duplex Designer";
    Layout = "~/Views/Shared/_HomeLayout.cshtml";
}
<!-- Start Main Banner Area -->
<div class="home-slides owl-carousel owl-theme">
    <div class="main-banner item-bg1 jarallax" data-jarallax='{"speed": 0.2}'>
        <div class="d-table">
            <div class="d-table-cell">
                <div class="container">
                    <div class="main-banner-content">
                        <h1>Building the homes of our neighbors</h1>
                        @*<p>Lorem ipsum dolor sit amet, consectetur adipiscing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua.</p>*@

                        <div class="btn-box">
                            <a href="~/Home/Contact" class="default-btn">Get a Free Quote <span></span></a>
                            <a href="~/Home/About" class="optional-btn">More About Us <span></span></a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="main-banner item-bg2 jarallax" data-jarallax='{"speed": 0.2}'>
        <div class="d-table">
            <div class="d-table-cell">
                <div class="container">
                    <div class="main-banner-content">
                        <h1>Design is a journey of discovery</h1>


                        <div class="btn-box">
                            <a href="~/Home/Contact" class="default-btn">Contact Us <span></span></a>
                            <a href="~/Home/About" class="optional-btn">More About Us <span></span></a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="main-banner item-bg3 jarallax" data-jarallax='{"speed": 0.2}'>
        <div class="d-table">
            <div class="d-table-cell">
                <div class="container">
                    <div class="main-banner-content">
                        <h1>Design is thinking made visual</h1>

                        <div class="btn-box">
                            <a href="~/Home/Contact" class="default-btn">Free Quote <span></span></a>
                            <a href="~/Home/About" class="optional-btn">About Us <span></span></a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- End Main Banner Area -->
<!-- Start About Area -->
<div class="about-area">
    <div class="container">
        <div class="row">
            <div class="col-lg-7 col-md-12">
                <div class="about-content">
                    <span class="sub-title">Who We Are</span>
                    <h2>We are  <span>Duplex Designer</span></h2>
                    <p>Duplex Designer was established in 2019 with the aim to construct standard, safe, economical, and aesthetically pleasing structures all over Bangladesh. Our projects range from residential buildings to restaurants, mosques, and commercial buildings. We are equipped with expert professionals, strong work ethics and principles, and an unfaltering determination to ensure supreme quality for all our clients.</p>
                    @*<div class="quote">
                            “Interior Design is making the best possible use of the Available Space
                        </div>*@
                    <a href="~/Home/About" class="default-btn">More About Us <span></span></a>

                    <div class="back-animation-text">Duplex Designer</div>
                </div>
            </div>

            <div class="col-lg-5 col-md-12">
                <div class="about-image">
                    <img src="~/Content/assets/img/Untitled design.png" alt="image">
                </div>
            </div>
        </div>
    </div>
</div>
<!-- End About Area -->
<!-- Start Offer Area -->
<section class="offer-area ptb-100">
    <div class="container">
        <div class="row">
            <div class="col-lg-4 col-md-6 col-sm-6">
                <div class="offer-box">
                    <div class="icon">
                        <i class="flaticon-curtain"></i>
                    </div>
                    <h3>Professional Design</h3>
                    <p>Professional design in building design ensures functionality, safety, aesthetics, and sustainability, creating well-designed structures for a positive user experience.</p>

                </div>
            </div>

            <div class="col-lg-4 col-md-6 col-sm-6">
                <div class="offer-box">
                    <div class="icon">
                        <i class="flaticon-desktop"></i>
                    </div>
                    <h3>Creative Desktop</h3>
                    <p>Creative desktop in building design fosters innovation, allowing designers to explore unique ideas, unconventional approaches, and imaginative solutions for exceptional architectural outcomes.</p>

                </div>
            </div>

            <div class="col-lg-4 col-md-6 col-sm-6 offset-lg-0 offset-md-3 offset-sm-3">
                <div class="offer-box">
                    <div class="icon">
                        <i class="flaticon-rulers"></i>
                    </div>
                    <h3>Skilled Architecture</h3>
                    <p>Skilled architecture in building design ensures expertise in structural integrity, spatial optimization, and aesthetic harmony, creating well-crafted and enduring structures.</p>

                </div>
            </div>
        </div>
    </div>
</section>
<!-- End Offer Area -->
<!-- Start Services Area -->
<section class="services-section ptb-100 jarallax" data-jarallax='{"speed": 0.3}'>
    <div class="container">
        <div class="section-title">
            <span class="sub-title">What We Do</span>
            <h2>We Provide Passion Fueled Interior and Exterior Services</h2>
            <p>We work towards developing beautiful, safe, and economical structures all around Bangladesh. Our projects are carried out in ways that meet deadlines and expectations of clients successfully as reflected through our slogan “pursuing the desire.”</p>
        </div>

        <div class="row">
            <div class="col-lg-4 col-md-6 col-sm-6">
                <div class="services-box">
                    <div class="icon">
                        <i class="flaticon-double-bed"></i>
                    </div>

                    <h3>ARCHITECTURE</h3>
                    <p>Architectural services include design, preparation of construction documents, and construction administration. <br />Best Architechture Design in Country. <br /></p>
                    <a href="~/Service/Architecture" class="read-more-btn">Read More</a>
                </div>
            </div>

            <div class="col-lg-4 col-md-6 col-sm-6">
                <div class="services-box">
                    <div class="icon">
                        <i class="flaticon-hotel"></i>
                    </div>

                    <h3>STRUCTURAL ENGINEERING</h3>
                    <p>Our team of structural engineers and designers leverage their “out of the box” creativity to develop and implement cost effective, intuitive, robust solutions for structural challenges.<br /></p>
                    <a href="~/Service/StructureEngineering" class="read-more-btn">Read More</a>
                </div>
            </div>

            <div class="col-lg-4 col-md-6 col-sm-6">
                <div class="services-box">
                    <div class="icon">
                        <i class="flaticon-living-room"></i>
                    </div>

                    <h3>ELECTRICAL WORKS</h3>
                    <p>We specialize in new construction, interior remodels, exterior remodels and building maintenance for retail, offices, financial institutions, healthcare facilities, educational facilities.</p>
                    <a href="~/Service/Electrical" class="read-more-btn">Read More</a>
                </div>
            </div>

            <div class="col-lg-4 col-md-6 col-sm-6">
                <div class="services-box">
                    <div class="icon">
                        <i class="flaticon-home"></i>
                    </div>

                    <h3>CONSTRUCTION WORKS</h3>
                    <p>We specialize in new construction, interior remodels, exterior remodels and building maintenance for retail, offices, financial institutions, healthcare facilities, educational facilities, government properties and residential complexes.</p>
                    <a href="~/Service/Construction" class="read-more-btn">Read More</a>
                </div>
            </div>

            <div class="col-lg-4 col-md-6 col-sm-6">
                <div class="services-box">
                    <div class="icon">
                        <i class="flaticon-kitchen"></i>
                    </div>

                    <h3>PLUMBING WORKS</h3>
                    <p>We specialize in new construction, interior remodels, exterior remodels and building maintenance for retail, offices, financial institutions, healthcare facilities, educational facilities, government properties and residential complexes.</p>
                    <a href="~/Service/Plumbing" class="read-more-btn">Read More</a>
                </div>
            </div>

            <div class="col-lg-4 col-md-6 col-sm-6">
                <div class="services-box">
                    <div class="icon">
                        <i class="flaticon-sketch"></i>
                    </div>

                    <h3>PROJECT MANAGEMENT</h3>
                    <p>Our project management services specialize in planning, coordinating, and executing projects according to specific requirements and constraints.We perform some or all of the activities related to project work.</p>
                    <a href="~/Service/ProjectManagement" class="read-more-btn">Read More</a>
                </div>
            </div>
        </div>
    </div>
</section>
<!-- End Services Area -->
